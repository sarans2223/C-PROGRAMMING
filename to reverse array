#include <stdio.h>
int main() {
int n,i,j=0,temp;
scanf("%d",&n);
int a[n];
for(i=0;i<n;i++){
    scanf("%d",&a[i]);
}
for(i=n-1;i>j;i--){
    temp=a[j]; //to store the very first elem
    a[j]=a[i]; //to convert the last element to first
    a[i]=temp; //to store the taken first element to new array which is going to be printed
    j++;}
    for(i=0;i<n;i++){
        printf("%d ",a[i]);
    } }
